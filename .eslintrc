{
    parser: 'babel-eslint',
    extends: 'airbnb',
    plugins: [
        'react'
    ],
    rules: {
        'linebreak-style': 0,
        'no-tabs': 0,
        'indent': 0,
        'react/jsx-indent': 0,
        'react/jsx-indent-props': 0,
        'react/jsx-filename-extension': 0,
        'react/destructuring-assignment': 0,
        'jsx-a11y/anchor-is-valid': 0,
        'no-underscore-dangle': 0,
        'no-useless-escape': 0,
        'react/forbid-prop-types': 0,

    },
    globals: {
        'window': true,
        '__DEV__': true
    }
}
